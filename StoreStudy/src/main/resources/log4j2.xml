<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<File name="fout" fileName="${sys:catalina.home}/logs/StoreStudy.log" append="true">
			<PatternLayout>
				<Pattern>"%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n">
				</Pattern>
			</PatternLayout>
		</File>

	</Appenders>
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="fout" />
		</Root>
	</Loggers>
</Configuration>